fn reverse8(n: i64) i64 {
    var result: i64 = 0;
    var x: i64 = n;
    var i: i64 = 0;
    while i < 8 {
        result = result << 1;
        result = result | (x & 1);
        x = x >> 1;
        i = i + 1;
    }
    return result;
}
fn main() i64 { if reverse8(0b10110000) == 0b00001101 { return 0; } return 1; }
