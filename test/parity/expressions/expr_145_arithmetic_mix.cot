fn main() i64 {
    var a: i64 = 10;
    var b: i64 = 3;
    var sum: i64 = a + b;
    var diff: i64 = a - b;
    var prod: i64 = a * b;
    var quot: i64 = a / b;
    var rem: i64 = a % b;
    var result: i64 = sum + diff + prod + quot + rem;
    if result == 54 { return 0; }  // 13+7+30+3+1=54
    return 1;
}
