extern fn malloc_i64(count: i64) *i64;
fn main() i64 {
    var mat: *i64 = malloc_i64(9);
    var i: i64 = 0;
    while i < 3 {
        var j: i64 = 0;
        while j < 3 {
            (mat + i * 3 + j).* = i * 3 + j;
            j = j + 1;
        }
        i = i + 1;
    }
    if (mat + 4).* == 4 and (mat + 8).* == 8 { return 0; }
    return 1;
}
