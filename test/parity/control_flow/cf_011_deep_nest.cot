fn deep_nest(x: i64) i64 {
    if x > 0 {
        if x > 5 {
            if x > 10 {
                if x > 15 { return 4; }
                return 3;
            }
            return 2;
        }
        return 1;
    }
    return 0;
}
test "control flow: deep nest" {
    @assert(deep_nest(12) == 3)
}
