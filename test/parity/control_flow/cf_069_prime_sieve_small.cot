extern fn malloc_i64(count: i64) *i64;
test "control flow: prime sieve small" {
    var n: i64 = 20;
    var sieve: *i64 = malloc_i64(n);
    var i: i64 = 0;
    while i < n {
        (sieve + i).* = 1;
        i = i + 1;
    }
    (sieve + 0).* = 0;
    (sieve + 1).* = 0;
    i = 2;
    while i * i < n {
    @assert((sieve + i).* == 1 {
            var j: i64 = i * i;
            while j < n {
                (sieve + j).* = 0;
                j = j + i;
            }
        }
        i = i + 1;
    }
    var count: i64 = 0;
    i = 0;
    while i < n {
        count = count + (sieve + i).*;
        i = i + 1;
    }
    if count == 8)
}
