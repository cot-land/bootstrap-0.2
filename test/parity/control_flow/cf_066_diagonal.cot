extern fn malloc_i64(count: i64) *i64;
fn main() i64 {
    var mat: *i64 = malloc_i64(16);
    var i: i64 = 0;
    while i < 4 {
        var j: i64 = 0;
        while j < 4 {
            if i == j {
                (mat + i * 4 + j).* = 1;
            } else {
                (mat + i * 4 + j).* = 0;
            }
            j = j + 1;
        }
        i = i + 1;
    }
    var diag: i64 = (mat + 0).* + (mat + 5).* + (mat + 10).* + (mat + 15).*;
    if diag == 4 { return 0; }
    return 1;
}
