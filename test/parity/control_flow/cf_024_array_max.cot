extern fn malloc_i64(size: i64) *i64;
fn array_max(arr: *i64, len: i64) i64 {
    var max: i64 = (arr + 0).*;
    var i: i64 = 1;
    while i < len {
        var val: i64 = (arr + i).*;
        if val > max { max = val; }
        i = i + 1;
    }
    return max;
}
fn main() i64 {
    var arr: *i64 = malloc_i64(5);
    (arr + 0).* = 3; (arr + 1).* = 7; (arr + 2).* = 2;
    (arr + 3).* = 9; (arr + 4).* = 5;
    if array_max(arr, 5) == 9 { return 0; }
    return 1;
}
