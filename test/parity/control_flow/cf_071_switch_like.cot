fn day_of_week(d: i64) i64 {
    if d == 0 { return 0; }
    if d == 1 { return 1; }
    if d == 2 { return 2; }
    if d == 3 { return 3; }
    if d == 4 { return 4; }
    if d == 5 { return 5; }
    if d == 6 { return 6; }
    return 0 - 1;
}
fn main() i64 {
    if day_of_week(3) == 3 and day_of_week(7) == 0 - 1 { return 0; }
    return 1;
}
