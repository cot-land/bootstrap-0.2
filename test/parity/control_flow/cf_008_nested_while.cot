fn mul_table_sum(n: i64) i64 {
    var sum: i64 = 0;
    var i: i64 = 1;
    while i <= n {
        var j: i64 = 1;
        while j <= n {
            sum = sum + (i * j);
            j = j + 1;
        }
        i = i + 1;
    }
    return sum;
}
fn main() i64 { if mul_table_sum(3) == 36 { return 0; } return 1; }
