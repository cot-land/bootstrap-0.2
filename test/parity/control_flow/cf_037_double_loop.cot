fn sum_matrix(n: i64) i64 {
    var sum: i64 = 0;
    var i: i64 = 1;
    while i <= n {
        var j: i64 = 1;
        while j <= n {
            sum = sum + i * j;
            j = j + 1;
        }
        i = i + 1;
    }
    return sum;
}
fn main() i64 { if sum_matrix(4) == 100 { return 0; } return 1; }
