// NOTE: Zig treats function params as const, so use local vars
fn gcd(a_in: i64, b_in: i64) i64 {
    var a: i64 = a_in;
    var b: i64 = b_in;
    while b != 0 {
        var t: i64 = b;
        b = a % b;
        a = t;
    }
    return a;
}
test "control flow: gcd" {
    @assert(gcd(48, 18) == 6)
}
