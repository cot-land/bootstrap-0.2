fn count_in_range(start: i64, end: i64, divisor: i64) i64 {
    var count: i64 = 0;
    var i: i64 = start;
    while i <= end {
        if i % divisor == 0 {
            count = count + 1;
        }
        i = i + 1;
    }
    return count;
}
test "control flow: count matches" {
    @assert(count_in_range(1, 100, 7) == 14)
}
