fn count_fizzbuzz(n: i64) i64 {
    var count: i64 = 0;
    var i: i64 = 1;
    while i <= n {
        if (i % 3) == 0 {
            if (i % 5) == 0 { count = count + 1; }
        }
        i = i + 1;
    }
    return count;
}
test "control flow: fizzbuzz count" {
    @assert(count_fizzbuzz(30) == 2)
}
