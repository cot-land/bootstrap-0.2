fn main() i64 {
    var sum: i64 = 0;
    var i: i64 = 0;
    while i < 2 {
        var j: i64 = 0;
        while j < 3 {
            var k: i64 = 0;
            while k < 4 {
                sum = sum + 1;
                k = k + 1;
            }
            j = j + 1;
        }
        i = i + 1;
    }
    if sum == 24 { return 0; }
    return 1;
}
