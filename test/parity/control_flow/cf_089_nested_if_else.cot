fn grade(score: i64) i64 {
    if score >= 90 {
        return 4;
    } else {
        if score >= 80 {
            return 3;
        } else {
            if score >= 70 {
                return 2;
            } else {
                if score >= 60 {
                    return 1;
                } else {
                    return 0;
                }
            }
        }
    }
}
fn main() i64 {
    if grade(85) == 3 { return 0; }
    return 1;
}
