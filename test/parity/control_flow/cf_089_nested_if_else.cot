fn grade(score: i64) i64 {
    if score >= 90 {
        return 4;
    } else {
        if score >= 80 {
            return 3;
        } else {
            if score >= 70 {
                return 2;
            } else {
                if score >= 60 {
                    return 1;
                } else {
                    return 0;
                }
            }
        }
    }
}
test "control flow: nested if else" {
    @assert(grade(85) == 3)
}
