extern fn malloc_i64(count: i64) *i64;
fn swap(arr: *i64, i: i64, j: i64) void {
    var t: i64 = (arr + i).*;
    (arr + i).* = (arr + j).*;
    (arr + j).* = t;
}
fn main() i64 {
    var arr: *i64 = malloc_i64(3);
    (arr + 0).* = 1;
    (arr + 1).* = 2;
    (arr + 2).* = 3;
    swap(arr, 0, 2);
    if (arr + 0).* == 3 and (arr + 2).* == 1 { return 0; }
    return 1;
}
