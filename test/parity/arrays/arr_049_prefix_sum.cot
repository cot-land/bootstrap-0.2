extern fn malloc_i64(count: i64) *i64;
fn prefix_sum(arr: *i64, n: i64) void {
    var i: i64 = 1;
    while i < n {
        (arr + i).* = (arr + i).* + (arr + i - 1).*;
        i = i + 1;
    }
}
test "array: prefix sum" {
    var arr: *i64 = malloc_i64(5);
    (arr + 0).* = 1;
    (arr + 1).* = 2;
    (arr + 2).* = 3;
    (arr + 3).* = 4;
    (arr + 4).* = 5;
    prefix_sum(arr, 5);
    @assert((arr + 4).* == 15)
}
