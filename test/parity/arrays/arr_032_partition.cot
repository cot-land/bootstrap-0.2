extern fn malloc_i64(count: i64) *i64;
fn count_less(arr: *i64, n: i64, pivot: i64) i64 {
    var count: i64 = 0;
    var i: i64 = 0;
    while i < n {
        if (arr + i).* < pivot {
            count = count + 1;
        }
        i = i + 1;
    }
    return count;
}
fn main() i64 {
    var arr: *i64 = malloc_i64(7);
    (arr + 0).* = 3;
    (arr + 1).* = 1;
    (arr + 2).* = 4;
    (arr + 3).* = 1;
    (arr + 4).* = 5;
    (arr + 5).* = 9;
    (arr + 6).* = 2;
    if count_less(arr, 7, 4) == 4 { return 0; }
    return 1;
}
