extern fn malloc_i64(count: i64) *i64;
fn count_value(arr: *i64, n: i64, val: i64) i64 {
    var count: i64 = 0;
    var i: i64 = 0;
    while i < n {
        if (arr + i).* == val {
            count = count + 1;
        }
        i = i + 1;
    }
    return count;
}
fn main() i64 {
    var arr: *i64 = malloc_i64(7);
    (arr + 0).* = 1;
    (arr + 1).* = 2;
    (arr + 2).* = 3;
    (arr + 3).* = 2;
    (arr + 4).* = 1;
    (arr + 5).* = 2;
    (arr + 6).* = 1;
    if count_value(arr, 7, 2) == 3 { return 0; }
    return 1;
}
