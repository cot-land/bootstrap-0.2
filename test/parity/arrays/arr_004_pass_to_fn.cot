extern fn malloc_i64(count: i64) *i64;
fn sum_array(arr: *i64, len: i64) i64 {
    var sum: i64 = 0;
    var i: i64 = 0;
    while i < len {
        sum = sum + (arr + i).*;
        i = i + 1;
    }
    return sum;
}
fn main() i64 {
    var arr: *i64 = malloc_i64(5);
    (arr + 0).* = 1;
    (arr + 1).* = 2;
    (arr + 2).* = 3;
    (arr + 3).* = 4;
    (arr + 4).* = 5;
    if sum_array(arr, 5) == 15 { return 0; }
    return 1;
}
