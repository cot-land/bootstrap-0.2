extern fn malloc_i64(count: i64) *i64;
fn is_sorted(arr: *i64, n: i64) i64 {
    var i: i64 = 1;
    while i < n {
        if (arr + i - 1).* > (arr + i).* {
            return 0;
        }
        i = i + 1;
    }
    return 1;
}
fn main() i64 {
    var arr: *i64 = malloc_i64(5);
    (arr + 0).* = 1;
    (arr + 1).* = 2;
    (arr + 2).* = 3;
    (arr + 3).* = 4;
    (arr + 4).* = 5;
    var r1: i64 = is_sorted(arr, 5);
    (arr + 2).* = 10;
    var r2: i64 = is_sorted(arr, 5);
    if r1 == 1 and r2 == 0 { return 0; }
    return 1;
}
