extern fn malloc_i64(count: i64) *i64;
fn average(arr: *i64, n: i64) i64 {
    var sum: i64 = 0;
    var i: i64 = 0;
    while i < n {
        sum = sum + (arr + i).*;
        i = i + 1;
    }
    return sum / n;
}
fn main() i64 {
    var arr: *i64 = malloc_i64(5);
    (arr + 0).* = 10;
    (arr + 1).* = 20;
    (arr + 2).* = 30;
    (arr + 3).* = 40;
    (arr + 4).* = 50;
    if average(arr, 5) == 30 { return 0; }
    return 1;
}
