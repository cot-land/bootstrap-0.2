extern fn malloc_i64(count: i64) *i64;
fn upper_bound(arr: *i64, n: i64, target: i64) i64 {
    var lo: i64 = 0;
    var hi: i64 = n;
    while lo < hi {
        var mid: i64 = (lo + hi) / 2;
        if (arr + mid).* <= target {
            lo = mid + 1;
        } else {
            hi = mid;
        }
    }
    return lo;
}
fn main() i64 {
    var arr: *i64 = malloc_i64(5);
    (arr + 0).* = 10;
    (arr + 1).* = 20;
    (arr + 2).* = 30;
    (arr + 3).* = 40;
    (arr + 4).* = 50;
    if upper_bound(arr, 5, 30) == 3 { return 0; }
    return 1;
}
