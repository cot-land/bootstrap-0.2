extern fn malloc_i64(count: i64) *i64;
fn copy_array(src: *i64, dst: *i64, len: i64) void {
    var i: i64 = 0;
    while i < len {
        (dst + i).* = (src + i).*;
        i = i + 1;
    }
}
fn main() i64 {
    var src: *i64 = malloc_i64(3);
    var dst: *i64 = malloc_i64(3);
    (src + 0).* = 100;
    (src + 1).* = 200;
    (src + 2).* = 300;
    copy_array(src, dst, 3);
    if (dst + 0).* == 100 and (dst + 1).* == 200 and (dst + 2).* == 300 { return 0; }
    return 1;
}
