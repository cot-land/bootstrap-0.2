extern fn malloc_i64(count: i64) *i64;
fn count_even(arr: *i64, n: i64) i64 {
    var count: i64 = 0;
    var i: i64 = 0;
    while i < n {
        if (arr + i).* % 2 == 0 {
            count = count + 1;
        }
        i = i + 1;
    }
    return count;
}
fn main() i64 {
    var arr: *i64 = malloc_i64(10);
    var i: i64 = 0;
    while i < 10 {
        (arr + i).* = i;
        i = i + 1;
    }
    if count_even(arr, 10) == 5 { return 0; }
    return 1;
}
