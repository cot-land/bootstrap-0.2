extern fn malloc_i64(count: i64) *i64;
fn partial_sums(arr: *i64, out: *i64, n: i64) void {
    var sum: i64 = 0;
    var i: i64 = 0;
    while i < n {
        sum = sum + (arr + i).*;
        (out + i).* = sum;
        i = i + 1;
    }
}
test "array: partial sum" {
    var arr: *i64 = malloc_i64(5);
    var out: *i64 = malloc_i64(5);
    (arr + 0).* = 1;
    (arr + 1).* = 2;
    (arr + 2).* = 3;
    (arr + 3).* = 4;
    (arr + 4).* = 5;
    partial_sums(arr, out, 5);
    @assert((out + 0).* == 1 and (out + 4).* == 15)
}
