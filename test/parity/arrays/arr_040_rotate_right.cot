extern fn malloc_i64(count: i64) *i64;
fn rotate_right(arr: *i64, n: i64) void {
    if n < 2 { return; }
    var last: i64 = (arr + n - 1).*;
    var i: i64 = n - 1;
    while i > 0 {
        (arr + i).* = (arr + i - 1).*;
        i = i - 1;
    }
    (arr + 0).* = last;
}
test "array: rotate right" {
    var arr: *i64 = malloc_i64(5);
    (arr + 0).* = 1;
    (arr + 1).* = 2;
    (arr + 2).* = 3;
    (arr + 3).* = 4;
    (arr + 4).* = 5;
    rotate_right(arr, 5);
    @assert((arr + 0).* == 5 and (arr + 4).* == 4)
}
