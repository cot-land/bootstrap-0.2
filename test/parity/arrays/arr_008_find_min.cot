extern fn malloc_i64(count: i64) *i64;
fn find_min(arr: *i64, len: i64) i64 {
    var min: i64 = (arr + 0).*;
    var i: i64 = 1;
    while i < len {
        if (arr + i).* < min {
            min = (arr + i).*;
        }
        i = i + 1;
    }
    return min;
}
fn main() i64 {
    var arr: *i64 = malloc_i64(5);
    (arr + 0).* = 30;
    (arr + 1).* = 10;
    (arr + 2).* = 50;
    (arr + 3).* = 5;
    (arr + 4).* = 40;
    if find_min(arr, 5) == 5 { return 0; }
    return 1;
}
