extern fn malloc_i64(count: i64) *i64;
fn find_last(arr: *i64, n: i64, val: i64) i64 {
    var last_idx: i64 = 0 - 1;
    var i: i64 = 0;
    while i < n {
        if (arr + i).* == val {
            last_idx = i;
        }
        i = i + 1;
    }
    return last_idx;
}
fn main() i64 {
    var arr: *i64 = malloc_i64(6);
    (arr + 0).* = 1;
    (arr + 1).* = 2;
    (arr + 2).* = 3;
    (arr + 3).* = 2;
    (arr + 4).* = 1;
    (arr + 5).* = 2;
    if find_last(arr, 6, 2) == 5 { return 0; }
    return 1;
}
