extern fn malloc_i64(count: i64) *i64;
fn find_max(arr: *i64, len: i64) i64 {
    var max: i64 = (arr + 0).*;
    var i: i64 = 1;
    while i < len {
        if (arr + i).* > max {
            max = (arr + i).*;
        }
        i = i + 1;
    }
    return max;
}
test "array: find max" {
    var arr: *i64 = malloc_i64(5);
    (arr + 0).* = 30;
    (arr + 1).* = 10;
    (arr + 2).* = 50;
    (arr + 3).* = 20;
    (arr + 4).* = 40;
    @assert(find_max(arr, 5) == 50)
}
