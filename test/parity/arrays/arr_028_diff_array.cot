extern fn malloc_i64(count: i64) *i64;
fn diff(arr: *i64, out: *i64, n: i64) void {
    var i: i64 = 0;
    while i < n - 1 {
        (out + i).* = (arr + i + 1).* - (arr + i).*;
        i = i + 1;
    }
}
test "array: diff array" {
    var arr: *i64 = malloc_i64(5);
    var out: *i64 = malloc_i64(4);
    (arr + 0).* = 1;
    (arr + 1).* = 3;
    (arr + 2).* = 6;
    (arr + 3).* = 10;
    (arr + 4).* = 15;
    diff(arr, out, 5);
    @assert((out + 0).* == 2 and (out + 3).* == 5)
}
