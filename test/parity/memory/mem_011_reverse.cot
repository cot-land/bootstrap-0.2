extern fn malloc_i64(size: i64) *i64;
fn reverse(arr: *i64, len: i64) void {
    var i: i64 = 0;
    while i < len / 2 {
        var t: i64 = (arr + i).*;
        (arr + i).* = (arr + len - 1 - i).*;
        (arr + len - 1 - i).* = t;
        i = i + 1;
    }
}
test "memory: reverse" {
    var arr: *i64 = malloc_i64(5);
    (arr + 0).* = 1; (arr + 1).* = 2; (arr + 2).* = 3;
    (arr + 3).* = 4; (arr + 4).* = 5;
    reverse(arr, 5);
    @assert((arr + 0).* == 5)
}
