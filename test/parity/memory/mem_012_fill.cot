extern fn malloc_i64(size: i64) *i64;
fn fill(arr: *i64, len: i64, val: i64) void {
    var i: i64 = 0;
    while i < len {
        (arr + i).* = val;
        i = i + 1;
    }
}
fn main() i64 {
    var arr: *i64 = malloc_i64(10);
    fill(arr, 10, 42);
    var sum: i64 = 0;
    var i: i64 = 0;
    while i < 10 {
        sum = sum + (arr + i).*;
        i = i + 1;
    }
    if sum == 420 { return 0; }
    return 1;
}
