extern fn malloc_i64(size: i64) *i64;
fn main() i64 {
    var arr: *i64 = malloc_i64(10);
    var i: i64 = 0;
    while i < 10 {
        (arr + i).* = i;
        i = i + 1;
    }
    var sum: i64 = 0;
    i = 0;
    while i < 10 {
        sum = sum + (arr + i).*;
        i = i + 1;
    }
    if sum == 45 { return 0; }
    return 1;
}
