extern fn malloc_i64(count: i64) *i64;
fn init_sequence(arr: *i64, n: i64) void {
    var i: i64 = 0;
    while i < n {
        (arr + i).* = i + 1;
        i = i + 1;
    }
}
fn main() i64 {
    var arr: *i64 = malloc_i64(5);
    init_sequence(arr, 5);
    if (arr + 4).* == 5 { return 0; }
    return 1;
}
