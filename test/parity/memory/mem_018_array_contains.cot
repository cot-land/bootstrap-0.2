extern fn malloc_i64(size: i64) *i64;
fn contains(arr: *i64, len: i64, val: i64) i64 {
    var i: i64 = 0;
    while i < len {
        if (arr + i).* == val { return 1; }
        i = i + 1;
    }
    return 0;
}
test "memory: array contains" {
    var arr: *i64 = malloc_i64(5);
    (arr + 0).* = 10; (arr + 1).* = 20; (arr + 2).* = 30;
    (arr + 3).* = 40; (arr + 4).* = 50;
    @assert(contains(arr, 5, 30) == 1)
}
