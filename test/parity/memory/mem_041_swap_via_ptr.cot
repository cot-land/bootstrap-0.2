extern fn malloc_i64(count: i64) *i64;
fn swap_ptr(a: *i64, b: *i64) void {
    var t: i64 = a.*;
    a.* = b.*;
    b.* = t;
}
fn main() i64 {
    var x: *i64 = malloc_i64(1);
    var y: *i64 = malloc_i64(1);
    x.* = 10;
    y.* = 20;
    swap_ptr(x, y);
    if x.* == 20 and y.* == 10 { return 0; }
    return 1;
}
