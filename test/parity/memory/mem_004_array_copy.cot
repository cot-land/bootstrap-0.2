extern fn malloc_i64(size: i64) *i64;
fn copy_array(src: *i64, dst: *i64, len: i64) void {
    var i: i64 = 0;
    while i < len {
        (dst + i).* = (src + i).*;
        i = i + 1;
    }
}
fn main() i64 {
    var a: *i64 = malloc_i64(5);
    var b: *i64 = malloc_i64(5);
    (a + 0).* = 1; (a + 1).* = 2; (a + 2).* = 3; (a + 3).* = 4; (a + 4).* = 5;
    copy_array(a, b, 5);
    if (b + 2).* == 3 { return 0; }
    return 1;
}
