struct Data { a: i64, b: i64, c: i64, d: i64 }
fn init_data(d: *Data, v: i64) void {
    d.a = v;
    d.b = v * 2;
    d.c = v * 3;
    d.d = v * 4;
}
test "memory: struct init all" {
    var d: Data = undefined;
    init_data(&d, 10);
    @assert(d.a == 10 and d.b == 20 and d.c == 30 and d.d == 40)
}
