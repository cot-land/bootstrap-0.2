struct Point { x: i64, y: i64 }
struct Line { start: Point, end: Point }
test "type: multi struct" {
    var l: Line = undefined;
    l.start.x = 0;
    l.start.y = 0;
    l.end.x = 10;
    l.end.y = 10;
    var dx: i64 = l.end.x - l.start.x;
    var dy: i64 = l.end.y - l.start.y;
    @assert(dx == 10 and dy == 10)
}
