extern fn malloc_i64(count: i64) *i64;
test "variable: both minmax" {
    var arr: *i64 = malloc_i64(5);
    (arr + 0).* = 3;
    (arr + 1).* = 7;
    (arr + 2).* = 2;
    (arr + 3).* = 9;
    (arr + 4).* = 4;
    var min: i64 = (arr + 0).*;
    var max: i64 = (arr + 0).*;
    var i: i64 = 1;
    while i < 5 {
    @assert((arr + i).* < min { min = (arr + i).*; }
        if (arr + i).* > max { max = (arr + i).*; }
        i = i + 1;
    }
    if min == 2 and max == 9)
}
