fn is_prime(n: i64) i64 {
    if n < 2 { return 0; }
    var i: i64 = 2;
    while i * i <= n {
        if (n % i) == 0 { return 0; }
        i = i + 1;
    }
    return 1;
}
fn count_primes(n: i64) i64 {
    var count: i64 = 0;
    var i: i64 = 2;
    while i <= n {
        if is_prime(i) != 0 { count = count + 1; }
        i = i + 1;
    }
    return count;
}
fn main() i64 { if count_primes(20) == 8 { return 0; } return 1; }
