// KNOWN BUG: cot1 has bug with nested recursive calls
// ack(m - 1, ack(m, n - 1)) - inner ack result is corrupted
// cot1 returns 6, should be 9 for ack(2,3)
fn ack(m: i64, n: i64) i64 {
    if m == 0 { return n + 1; }
    if n == 0 { return ack(m - 1, 1); }
    return ack(m - 1, ack(m, n - 1));
}
fn main() i64 { if ack(2, 3) == 9 { return 0; } return 1; }
