fn is_prime(n: i64) i64 {
    if n <= 1 { return 0; }
    if n <= 3 { return 1; }
    if n % 2 == 0 { return 0; }
    var i: i64 = 3;
    while i * i <= n {
        if n % i == 0 { return 0; }
        i = i + 2;
    }
    return 1;
}
fn nth_prime(n: i64) i64 {
    var count: i64 = 0;
    var num: i64 = 2;
    while count < n {
        if is_prime(num) == 1 {
            count = count + 1;
            if count == n { return num; }
        }
        num = num + 1;
    }
    return num;
}
fn main() i64 {
    if nth_prime(10) == 29 { return 0; }
    return 1;
}
